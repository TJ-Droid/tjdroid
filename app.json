{
  "expo": {
    "name": "TJ Droid",
    "slug": "tjdroid",
    "scheme": "tjdroid",
    "version": "1.1.4",
    "jsEngine": "hermes",
    "assetBundlePatterns": ["**/*"],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0277bd"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "dev.pedropaulo.tjdroid",
      "useNextNotificationsApi": true,
      "versionCode": 24,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0277bd"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "name": "TJ Droid"
}
